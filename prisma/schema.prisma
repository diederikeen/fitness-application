generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            	Int           @id @default(autoincrement())
  email         	String        @unique
  firstName     		String
  lastName      	String
  photoUrl      	String?
  streetName    	String?
  zipcode       	String?
  city          	String?
  country       	String?
  uid							String				@unique
}